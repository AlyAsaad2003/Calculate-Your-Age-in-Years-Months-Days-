#include <iostream>
#include <string>
#include <algorithm>
#include <iomanip>
using namespace std;
const double PI = 3.141592653;
void init() {

    ios_base::sync_with_stdio(0);
    cin.tie(0);
    cout.tie(0);
}
int main()
{
    init();
    int N, year, month, day;
    cin >> N;
    year = N / 365;
    month = N % 365 / 30;
    day = N % 365 % 30;
    cout << year << " years\n";
    cout << month << " months\n";
    cout << day << " days\n";
    return 0;
}
